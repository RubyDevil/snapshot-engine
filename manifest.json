{
  "manifest_version": 3,
  "name": "Snapshot Engine",
  "short_name": "Snapshot",
  "description": "A powerful browser extension to take snapshots of a JavaScript object and compare them to see what properties have changed.",
  "version": "1.0.1",
  "permissions": [],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "world": "MAIN",
      "run_at": "document_start",
      "matches": ["*://*/*"],
      "all_frames": false,
      "match_about_blank": false
    }
  ],
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
}
